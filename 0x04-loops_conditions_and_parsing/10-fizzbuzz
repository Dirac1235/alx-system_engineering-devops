#!/usr/bin/env bash
# prints fizz buzz based on multiplicites

for i in {1..100}
do
    if [ $((i % 3)) -eq 0 -a $((i % 5)) -eq 0 ]
    then
        echo "FizzBuzz"
    elif [ $((i % 3)) -eq 0 ]
    then
        echo "Fizz"
    elif [ $((i % 5)) -eq 0 ]
    then
        echo "Buzz"
    else
        echo $i
    fi
done
